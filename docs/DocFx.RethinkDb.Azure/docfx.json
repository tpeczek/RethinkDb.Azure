{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/RethinkDb.Azure.WebJobs.Extensions/RethinkDb.Azure.WebJobs.Extensions.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../.."
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "toc.md",
          "index.md",
          "articles/azure-functions-bindings-rethinkdb.md",
          "articles/azure-rethinkdb-architectures.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/ico/favicon.ico",
          "resources/svg/logo.svg",
          "resources/svg/azure-rethinkdb-function-apps-architecture.svg"
        ]
      }
    ],
    "dest": "wwwroot",
    "globalMetadata": {
      "_appTitle": "RethinkDb.Azure",
      "_appFooter": "Copyright © 2018 - 2024 Tomasz Pęczek",
      "_appLogoPath": "resources/svg/logo.svg",
      "_appFaviconPath": "resources/ico/favicon.ico",
      "_disableBreadcrumb": true,
      "_disableAffix": true,
      "_disableContribution": true
    }
  }
}